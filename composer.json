{
    "name": "edisonlabs/merge-yaml",
    "type": "composer-plugin",
    "description": "Provides a composer plugin which merges yaml files.",
    "license": "GPL-2.0+",
    "autoload": {
        "psr-4": {
            "EdisonLabs\\MergeYaml\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": "true",
    "require": {
        "composer-plugin-api": "^1.1",
        "symfony/finder": "^4",
        "symfony/yaml": "^4"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "composer/composer": "dev-master",
        "php-coveralls/php-coveralls": "^2.0"
    },
    "extra": {
        "class": "EdisonLabs\\MergeYaml\\Plugin"
    }
}
